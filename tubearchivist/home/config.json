{
    "archive": {
        "sort": "published",
        "filter_view": "pending",
        "hide_watched": false,
        "show_subed_only": false,
        "page_size": 12
    },
    "subscriptions": {
        "auto_search": false,
        "auto_download": false,
        "channel_size": 50
    },
    "downloads": {
        "limit_count": false,
        "limit_speed": false,
        "sleep_interval": 3,
        "format": false,
        "add_metadata": false
    },
    "application": {
        "cache_dir": "/cache",
        "videos": "/youtube",
        "file_template": "%(id)s_%(title)s.mp4",
        "colors": "dark"
    }
}
